            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://www.w3.org/ns/prov#Agent> ;
            owl:onProperty <http://www.w3.org/ns/prov#actedOnBehalfOf> ] ;
    skos:definition "a provence-generating agent" ;
